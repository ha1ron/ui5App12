{
	"_version": "1.7.0",
	"sap.app": {
		"id": "APP12FirstLook",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"Z00_APP12_SRV": {
				"uri": "/sap/opu/odata/sap/Z00_APP12_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "APP12FirstLook.view.RootView",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "APP12FirstLook.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z00_APP12_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}, {
				"uri": "css/smalStyle.css",
				"id": ""
			}]
		},
		"routing": {
			"config": {
				"viewPath": "APP12FirstLook.view",
				"controlId": "v_root",
				"viewLevel": 1,
				"controlAggregation": "pages",
				"routerClass": "sap.m.routing.Router"
			},
			"targets": {
				"TargetHeader": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewLevel": 1,
					"viewName": "V_Header"
				},
				"TargetDetails": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Details",
					"viewLevel": 2
				}
			},
			"routes": [{
				"name": "TargetHeader",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetHeader"]
			}, {
				"name": "TargetDetails",
				"pattern": "ZRGP_120NSet/{inputValue}/{endStation}/{code}",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetDetails"]
			}]
		}
	}
}